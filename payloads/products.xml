<MarketsCollection>
  <Market MarketID="{{MARKET_ID}}" CatalogSize="{{CATALOG_SIZE}}">
    <ProductsUpdate>
      <Product ProductID="{{PRODUCT_ID}}" ProductName="{{PRODUCT_NAME}}" Price="{{PRICE}}" Cost="{{COST}}" ProductCode="{{PRODUCT_CODE}}" Category="{{CATEGORY}}">
        <Codes>
          <Code>{{BARCODE}}</Code>
        </Codes>
        <Taxes>
          <Tax ID="{{TAX_ID}}" Name="{{TAX_NAME}}" Rate="{{TAX_RATE}}" IncludedInPrice="{{TAX_INCLUDED}}" />
        </Taxes>
        <Fees>
          <Fee ID="{{FEE_ID}}" Name="{{FEE_NAME}}" Value="{{FEE_VALUE}}" IsTaxable="{{FEE_TAXABLE}}" />
        </Fees>
      </Product>
    </ProductsUpdate>
  </Market>
</MarketsCollection>
